# WVOID-FM Configuration

# Music sources (list of directories containing audio files)
music_sources:
  - /path/to/your/music
  - /path/to/more/music

# Icecast settings
icecast:
  host: localhost
  port: 8000
  mount: /stream
  source_password: YOUR_SOURCE_PASSWORD

# Cloudflare tunnel (optional)
tunnel:
  name: wvoid-radio
  
# API server
api:
  port: 8001
